{
  "schemaVersion": 1,
  "id": "furnish",
  "version": "${version}",
  "name": "Furnish",
  "description": "Adds lovely and useful Furniture to Minecraft. Credits -- Book Pile model from the 'Comfy Cozy' mod (MIT Licensed). Bookshelf Chest textures from 'Charm' mod (MIT Licensed). Brazilian translations by mayrotiem. Chinese translations by Pancakes0228.",
  "authors": [
    "Wouink"
  ],
  "contact": {
    "homepage": "https://wouink.github.io",
    "sources": "https://github.com/wouink/furnish"
  },
  "license": "CC0-1.0",
  "icon": "assets/furnish/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "io.github.wouink.furnish.fabric.FurnishFabric"
    ],
    "client": [
      "io.github.wouink.furnish.fabric.FurnishFabricClient"
    ]
  },
  "mixins": [
    "furnish.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.16.0",
    "minecraft": "~1.21",
    "java": ">=21",
    "architectury": ">=13.0.2",
    "fabric-api": "*"
  }
